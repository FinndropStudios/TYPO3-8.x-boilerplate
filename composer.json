{
  "name": "fds/typo3-8.x-boilerplate",
  "license": "MIT",
  "type": "project",
  "description": "TYPO3 8.x Boilerplate (including mask & mask_export)",
  "authors": [
    {
      "name": "Manuel Bachl",
      "email": "m.bachl@finndrop.de"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "php": "^7.0.0",
    "typo3/cms": "8.5.1 as 8.2.0",
    "helhum/typo3-console": "^4.0",
    "dmitryd/typo3-realurl": "^2.1",
    "georgringer/news": "^5.3",
    "mrohnstock/cookieconsent2": "^0.2",
    "t3monitor/t3monitoring": "^1"
  },
  "autoload": {
    "psr-4": {
      "Finndrop\\WebsiteTemplate\\": "web/typo3conf/ext/website_template/Classes"
    }
  },
  "require-dev": {
    "mask/mask": "^2.1",
    "cpsit/mask-export": "^0",
    "ninecells/dev-log": "^0"
  },
  "autoload-dev": {

  },
  "scripts": {

  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "web/{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    },
    "helhum/typo3-console": {
      "install-binary": true,
      "install-extension-dummy": false
    }
  },
}